{
  "name": "TEC_Q5EP",
  "image": "mcr.microsoft.com/devcontainers/python:3.11-bullseye",
  "features": {
    "ghcr.io/devcontainers/features/node:1": { "version": "20" }
  },
  "postCreateCommand": "pip install -r requirements.txt && npm ci --prefix ui || true",
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python","ms-toolsai.jupyter","esbenp.prettier-vscode"]
    }
  },
  "remoteUser": "vscode"
}
