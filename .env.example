# TEC_Q5EP Environment Variables
# Copy this to .env and fill in your actual values
# Never commit .env to git - it's in .gitignore

# Azure AI Services
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_KEY=your-azure-openai-key
AZURE_OPENAI_VERSION=2024-05-01-preview

# Bitwarden Secrets Manager
BW_CLIENTID=your-bitwarden-client-id
BW_CLIENTSECRET=your-bitwarden-client-secret
BW_ACCESS_TOKEN=your-secrets-manager-access-token

# API Configuration
API_HOST=localhost
API_PORT=8000
API_KEY=your-api-secret-key

# Database (if using one)
DATABASE_URL=sqlite:///./tec_q5ep.db

# Environment
ENVIRONMENT=development
DEBUG=true